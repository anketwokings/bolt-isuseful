{
    "name": "twokings/is-useful",
    "description": "Add a simple 'Is this page useful?' to your pages",
    "type": "bolt-extension",
    "keywords": [
        "useful",
        "feedback"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Xiao-Hu Tai",
            "email": "xiao@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\TwoKings\\IsUseful\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\TwoKings\\IsUseful\\Extension",
        "bolt-icon": "bolt-icon.png"
    },
    "suggest": {
        "bolt/boltforms": "A form generator for Bolt CMS"
    }
}
